PROMPT=""
RPROMPT="%~"

# Specify default editor. Possible values: vim, nano, ed etc.
export EDITOR=vim

# git aliases
alias g="git"
alias ginit="git init ."
alias gadd="git add -all"
alias gc="git commit -v -a -m" 

# other aliases
alias clean="make clean"
alias run="make run"

alias ~="cd ~"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias ls="ls -F"
alias ll="ls -lah"

alias cdesk="cd ~/Desktop"
alias cdoc="cd ~/Documents"
alias clr="clear"
